<!DOCTYPE html>
<html>
<head>
  <title>CatsRedo</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

	<% if current_user %>
	  You are currently signed in as: "<%= current_user.username %>"
    <%= button_to "Log Out", session_url, :method => :delete%>
	<% else %>
	   <%= button_to "Log In", new_session_url, :method => :get%>
		 <%= button_to "Register", new_user_url, :method => :get%>
	<% end %>

  <%= yield %>

</body>
</html>
